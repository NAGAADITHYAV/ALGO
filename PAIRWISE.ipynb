   "source": [
    "n=int(input())\n",
    "a=[int(x) for x in input().split()]\n",
    "ma,max2=0,0\n",
    "for i in range(n):\n",
    "    if a[i]>ma:\n",
    "        if max2<ma:\n",
    "            max2=ma\n",
    "        ma=a[i]\n",
    "    elif a[i]<=ma and a[i]>max2:\n",
    "        max2=a[i]\n",
    "print(ma*max2)\n"
   ]
